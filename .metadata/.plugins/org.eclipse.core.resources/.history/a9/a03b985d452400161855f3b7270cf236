package eae;

public class Frog {
	private String leg;
	private String name;
	private int smarts;

	public String getLeg() {
		return leg;
	}

	public void setLeg(String leg) {
		this.leg = leg;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getSmarts() {
		return smarts;
	}

	public void setSmarts(int smarts) {
		this.smarts = smarts;
	}

}
